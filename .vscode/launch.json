    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "Start Debugging Callbacks",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}/package/callback/dist/index.js",
            "outFiles": [
                "${workspaceFolder}/package/callback/dist/**/*.js"
            ]
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Start Debugging Promises",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}/package/promise/dist/index.js",
            "outFiles": [
                "${workspaceFolder}/package/promise/dist/**/*.js"
            ]
        }
    ]
}