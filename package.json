        "build": "tsc",
        "start": "node dist/index.js",
        "debug": "nodemon --watch '*.ts' --exec 'ts-node' src/index.ts",
        "lint": "eslint '*/**/*.ts'",
        "lint-fix": "eslint '*/**/*.ts' --fix",
        "check-run": "npm run lint && npm run debug",
