        "mk713-crypto": "^1.0.2",
        "mk713-dynamo": "^1.1.0",
        "mk713-email": "^1.0.4",
        "mk713-repository": "^1.0.7",
        "mk713-server": "^1.0.6",
        "mk713-security": "^1.0.1",
        "uuid": "^9.0.0"
    },
